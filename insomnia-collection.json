    {
      "_id": "req_auth_login",
      "parentId": "fld_auth",
      "modified": "2025-12-14T18:55:51Z",
      "created": "2025-12-14T18:55:51Z",
      "url": "{{ base_url }}/auth/login",
      "name": "Login",
      "description": "",
      "method": "POST",
      "body": {
        "mimeType": "application/json",
        "text": "{\n  \"email\": \"ahmed@test.com\",\n  \"password\": \"Test@123456\"\n}"
      },
      "parameters": [],
      "headers": [
        {
          "name": "Content-Type",
          "value": "application/json"
        }
      ],
      "authentication": {},
      "metaSortKey": -1,
      "isPrivate": false,
      "_type": "request",
      "scripts": {
        "afterResponse": "const code = insomnia.response.code;\nif (code >= 200 && code < 300) {\n  try {\n    const body = insomnia.response.json();\n    const token = body?.access_token || body?.accessToken || body?.token;\n    if (token) {\n      insomnia.environment.set('access_token', token);\n      console.log('✅ Access token saved:', token.substring(0, 20) + '...');\n    }\n    \n    // Extract company_id from user object\n    const companyId = body?.user?.companyId || body?.companyId;\n    if (companyId) {\n      insomnia.environment.set('company_id', companyId);\n      console.log('✅ Company ID saved:', companyId);\n    }\n    \n    // Extract user_id from user object\n    const userId = body?.user?.id || body?.userId;\n    if (userId) {\n      insomnia.environment.set('user_id', userId);\n      console.log('✅ User ID saved:', userId);\n    }\n    \n    if (!token) {\n      console.log('❌ No token found in response:', JSON.stringify(body));\n    }\n  } catch (e) {\n    console.log('❌ Error parsing response:', e.message);\n  }\n} else {\n  console.log('❌ Login failed with status:', code);\n}"
      }
    },
